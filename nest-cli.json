{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "nex-libary": {
      "type": "library",
      "root": "libs/nex-libary",
      "entryFile": "index",
      "sourceRoot": "libs/nex-libary/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nex-libary/tsconfig.lib.json"
      }
    }
  }
}
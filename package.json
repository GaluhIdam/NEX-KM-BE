{
    "name": "nex-km-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "seed": "ts-node prisma/seed.ts",
        "build": "nest build",
        "cli": "",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:kuy": "nest start --watch",
        "start:dev": "concurrently \"nest build --webpack --webpackPath webpack-hmr.config.js --watch\" --raw \"node cli-script.js\"",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prisma:generate:nex-library": "npx prisma generate --schema prisma/nex-library.prisma",
        "prisma:generate:nex-community": "npx prisma generate --schema prisma/nex-community.prisma",
        "prisma:generate:nex-level": "npx prisma generate --schema prisma/nex-level.prisma",
        "prisma:generate:homepage": "npx prisma generate --schema prisma/homepage.prisma",
        "prisma:generate:nex-learning": "npx prisma generate --schema prisma/nex-learning.prisma",
        "prisma:generate:nex-talk": "npx prisma generate --schema prisma/nex-talk.prisma",
        "migrasi:semua": "concurrently \"node cli-script.js\"",
        "migrasi:semua-data": "concurrently \"node cli-script.js\" \"npx prisma db push --accept-data-loss --force-reset --schema=prisma/homepage.prisma && npx prisma db push --accept-data-loss --force-reset --schema=prisma/nex-community.prisma && npx prisma db push --accept-data-loss --force-reset --schema=prisma/nex-learning.prisma && npx prisma db push --accept-data-loss --force-reset --schema=prisma/nex-level.prisma && npx prisma db push --accept-data-loss --force-reset --schema=prisma/nex-library.prisma && npx prisma db push --accept-data-loss --force-reset --schema=prisma/nex-talk.prisma && yarn seed \""
    },
    "dependencies": {
        "@elastic/elasticsearch": "^8.10.0",
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@nestjs/bull": "^0.6.3",
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/elasticsearch": "^10.0.1",
        "@nestjs/mapped-types": "^2.0.2",
        "@nestjs/microservices": "^9.4.0",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.4.1",
        "@prisma/client": "^4.13.0",
        "bull": "^4.10.4",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "concurrently": "^8.2.0",
        "exceljs": "^4.4.0",
        "fluent-ffmpeg": "^2.1.2",
        "fs-extra": "^11.1.1",
        "fuse.js": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "keycloak-connect": "^21.1.1",
        "multer": "^1.4.5-lts.1",
        "nest-keycloak-connect": "^1.9.1",
        "nestjs-eureka": "^2.0.0",
        "reflect-metadata": "^0.1.13",
        "run-script-webpack-plugin": "^0.2.0",
        "rxjs": "^7.2.0",
        "sanitize-html": "^2.11.0",
        "uuid": "^9.0.0",
        "webpack-node-externals": "^3.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/express": "^4.17.13",
        "@types/fluent-ffmpeg": "^2.1.21",
        "@types/jest": "29.2.4",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.8.3",
        "@types/sanitize-html": "^2.9.0",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "29.3.1",
        "prettier": "^2.3.2",
        "prisma": "^4.13.0",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "29.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.1.1",
        "typescript": "^5.2.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
